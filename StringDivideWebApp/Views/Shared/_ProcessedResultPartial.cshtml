@model StringDivideModel

@if (Model != null && !string.IsNullOrEmpty(Model.ProcessedResult))
{
    <div id="displayinput" class="mt-3">
        <h4>Input Array</h4>
        <p>@Model.InputArray</p>
    </div>
    <div id="processedResultContainer" class="mt-3">
        <h4>Processed Result:</h4>
        <p>@Model.ProcessedResult</p>
    </div>
}